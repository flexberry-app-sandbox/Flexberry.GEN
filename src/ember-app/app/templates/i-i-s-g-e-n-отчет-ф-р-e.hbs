  <div class="field">
    <label>{{t "forms.i-i-s-g-e-n-отчет-ф-р-e.данныеОрг-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.данныеОрг
      displayAttributeName="email"
      autocomplete=true
      relationName="данныеОрг"
      projection="ДанныеОргL"
      title=(t "forms.i-i-s-g-e-n-отчет-ф-р-e.данныеОрг-caption")
      readonly=readonly
      componentName="данныеОргLookup"
      class=(if (v-get validationModel "данныеОрг" "isInvalid") "error")
      data-test-i-i-s-g-e-n-отчет-ф-р-e-данныеОрг=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "данныеОрг" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-g-e-n-отчет-ф-р-e.сотрудники-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудники
      displayAttributeName="фИО"
      autocomplete=true
      relationName="сотрудники"
      projection="СотрудникиL"
      title=(t "forms.i-i-s-g-e-n-отчет-ф-р-e.сотрудники-caption")
      readonly=readonly
      componentName="сотрудникиLookup"
      class=(if (v-get validationModel "сотрудники" "isInvalid") "error")
      data-test-i-i-s-g-e-n-отчет-ф-р-e-сотрудники=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудники" "message")}}
  </div>
</form>
