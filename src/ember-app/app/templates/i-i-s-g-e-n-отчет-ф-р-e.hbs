    {{flexberry-validationmessage error=(v-get validationModel "датаНачала" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерСтроки
      class=(if (v-get validationModel "номерСтроки" "isInvalid") "error")
      label=(t "forms.i-i-s-g-e-n-отчет-ф-р-e.номерСтроки-caption")
      data-test-i-i-s-g-e-n-отчет-ф-р-e-номерСтроки=true
