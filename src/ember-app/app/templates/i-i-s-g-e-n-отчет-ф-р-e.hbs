  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.датаНачала
      class=(if (v-get validationModel "датаНачала" "isInvalid") "error")
      label=(t "forms.i-i-s-g-e-n-отчет-ф-р-e.датаНачала-caption")
      data-test-i-i-s-g-e-n-отчет-ф-р-e-датаНачала=true
